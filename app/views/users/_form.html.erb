<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <br>
  <br>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>
  <br>
  <%= f.submit %>

<% end %>