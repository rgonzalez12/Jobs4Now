<h1>Create Event</h1>

<%= form_for @event do |f| %>
  <%= f.label :event_time %>
  <%= f.text_field :event_time %>
  <br>
  <br>
  <%= f.label :name %>
  <%= f.text_field :name %>
   <br>
   <br>
   <%= f.label :description %>
   <%= f.text_field :description %>
  <br>
  <br>
   <%= f.label :agenda %>
   <%= f.text_field :agenda %>
<br>
<br>
  <%= f.submit %>

<% end %>