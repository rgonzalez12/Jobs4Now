<h1>Login</h1>
<%= flash[:error] if flash[:error]%>
<br>
<br>
<%=  form_for :user do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Login" %>
  <% end %>

<%= button_to('Log in with Facebook', '/auth/facebook') %>